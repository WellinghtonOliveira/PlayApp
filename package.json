{
  "name": "playapp",
  "version": "1.0.0-beta.1",
  "description": "Um aplicativo que salva os dados da sua serie ou filme, salvando onde voce parou e em quantos minutos",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "author": "Wellinghton",
  "license": "ISC",
  "devDependencies": {
    "electron": "^38.4.0",
    "electron-builder": "^26.8.1",
    "electron-packager": "^17.1.2",
    "electron-reload": "^2.0.0-alpha.1"
  },
  "build": {
    "appId": "com.playapp.app",
    "productName": "PlayApp",
    "files": [
      "**/*",
      "!renderer/videos/**/*"
    ],
    "win": {
      "target": "nsis"
    }
  }
}